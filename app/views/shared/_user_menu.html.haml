%ul.unstyled.b-userpanel.pos-a.pos-top.dark-links
  - if account_signed_in?
    %li
      = link_to t(:profile), profile_path
      = t(:or)
      = link_to t(:logout), destroy_account_session_path
  - else
    %li
      = link_to t(:login), new_account_session_path
      = t(:or)
      = link_to t(:register), new_account_registration_path
    -#%li
      = t(:stop)
      = link_to t(:why_register), '#'
