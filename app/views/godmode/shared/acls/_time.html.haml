= f.label :acl_time_start, t(".time_range")
.input
	.inline-inputs
		= f.text_field :acl_time_start, :type => "time", :placeholder => "00:00", :class => "mini a-center"
		&mdash;
		= f.text_field :acl_time_end, :type => "time", :placeholder => "24:00", :class => "mini a-center"
	%span.help-block= t('.help.time_range')